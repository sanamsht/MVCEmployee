@model IEnumerable<MVCEmployee.Models.Excel.Customer>

   <div class="container">
       <div class="row">
           <div class="col-8 offset-2">
               <table class="table table-bordered">
                   <thead>
                       <tr>
                           <th>Cust_Id</th>
                           <th>First Name</th>
                           <th>Last Name</th>
                           <th>Gender</th>
                           <th>Age</th>
                           <th>Country</th>
                       </tr>
                   </thead>
                   <tbody>
                       @foreach(var d in Model)
                    {
                        <tr>
                            <td>@d.CustomerCode</td>
                            <td>@d.FirstName</td>
                            <td>@d.LastName</td>
                            <td>@d.Gender</td>
                            <td>@d.Age</td>
                            <td>@d.Country</td>
                        </tr>
                    }
                </tbody>
               </table>
           </div>
       </div>"
   </div>